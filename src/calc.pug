doctype html
html
head
    title="ЦК ГРУПП"
    meta(charset='utf-8')
    meta(http-equiv="X-UA-Compatible", content="IE=edge,chrome=1")
    meta(name='description', content="")
    meta(name='keywords', content="")
    meta(name="author", content="")
    meta(name="viewport", content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=yes, viewport-fit=cover")
    link(rel="icon" type="image/x-icon" href="./favicon.ico")
    link(rel="stylesheet" href="https://cdn.jsdelivr.net/npm/intl-tel-input@24.1.1/build/css/intlTelInput.css")
    link(rel="stylesheet", href="https://cdnjs.cloudflare.com/ajax/libs/aos/2.3.4/aos.css")
    link(rel="stylesheet", href="./main.css")
body
    .wrap
        header.header
            .container
                .header__row
                    a(href="/").logo
                        img(src="./img/logo.png", alt="ЦК ГРУПП")
                    ul.menu
                        //- li.menu__item
                        //-     a(href='#').menu__link Услуги
                        li.menu__item
                            a(href='calc.html').menu__link Калькулятор
                        li.menu__item
                            a(href='equipment.html').menu__link Оборудование
                        li.menu__item
                            a(href='about.html').menu__link О нас
                        li.menu__item
                            button(type='button').btn.application Оставить заявку
                    button(type='button').btn.header__btn.application Оставить заявку
                    button(type='button').burger
                        span
                        span
                        span
        main.main
            section.calc-page
                .container
                    .calc-page__row
                        h2 Калькулятор доходности
                        .calc-page__form
                            .select
                                select#equipment(onchange="setEquipment()")
                                    option Выберите оборудование
                                    option(data-consumption="2000", data-hashrate="200") Miner 1
                                    option(data-consumption="4000", data-hashrate="400") Miner 2
                                    option(data-consumption="2000", data-hashrate="200") Miner 3
                                    option(data-consumption="4000", data-hashrate="400") Miner 4
                            div.calc-page__item
                                label(for="electricityCost") Стоимость электричества (RUB/квт)
                                input#electricityCost(type="number", min="0.1", value="10")
                            div.calc-page__item
                                label(for="hashrate") Мощность (TH/s)
                                input#hashrate(type="number", value="200", min="1")
                            div.calc-page__item
                                label(for="consumption") Потребление (вт/час)
                                input#consumption(type="number", min="1", value="100")
                            div.calc-page__item
                                label(for="rateUSDRUB") Курс USD/RUB
                                input#rateUSDRUB(type="number", min="1", value="100" readonly)
                            div.calc-page__item
                                label(for="rateBTC") Курс BTC/USD
                                input#rateBTC(type="number", min="1", value="60000" readonly)
                            div.calc-page__item
                                label(for="commission") Комиссия (%)
                                input#commission(type="number", min="0", value="0")
                        .result#result
                                .result__wrap
                                    .result__data
                                        p День
                                        p Неделя
                                        p Месяц
                                        p Доход
                                        p#dayIncome 1
                                        p#weekIncome 7
                                        p#monthIncome 30
                                        p Расход
                                        p#dayOutGo 1
                                        p#weekOutGo 7
                                        p#monthOutGo 30
                                    .currency#currency
                                        p Валюта
                                        .currency__btns
                                            button.currency__btn.active(type="button")(data-rateUSD="1")(data-sign="$")
